{
  "name": "@myparcel/delivery-options",
  "version": "6.0.0-alpha.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/myparcelnl/delivery-options.git",
    "directory": "apps/delivery-options"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./ts": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "./src",
    "./dist"
  ],
  "scripts": {
    "build": "run build:default && run build:myparcel && run build:myparcel-lib",
    "build:default": "nx exec -- vite build",
    "build:default:dev": "nx exec -- run build:default -- --mode development",
    "build:dev": "run build:default:dev && run build:myparcel:dev && run build:myparcel-lib:dev",
    "build:myparcel": "nx exec -- vite build -c vite-myparcel.config.ts",
    "build:myparcel-lib": "nx exec -- vite build -c vite-myparcel-lib.config.ts",
    "build:myparcel-lib:dev": "nx exec -- run build:lib -- --mode development",
    "build:myparcel:dev": "nx exec -- run build:lib -- --mode development",
    "clean": "run ws:clean \"$(pwd)\"",
    "lint": "run ws:lint \"$(pwd)\"",
    "lint:fix": "run ws:lint:fix \"$(pwd)\"",
    "serve": "vite",
    "story:build": "histoire build",
    "story:dev": "histoire dev",
    "story:preview": "histoire preview",
    "test": "run ws:test \"$(pwd)\"",
    "test:coverage": "run ws:test:coverage \"$(pwd)\"",
    "test:run": "run ws:test:run \"$(pwd)\"",
    "test:update": "run ws:test:update \"$(pwd)\"",
    "typecheck": "vue-tsc --project tsconfig.base.json --noEmit"
  },
  "dependencies": {
    "@myparcel-do/shared": "workspace:*",
    "@myparcel/constants": "^1.1.0",
    "@myparcel/sdk": "^3.0.0",
    "@myparcel/ts-utils": "^1.12.0",
    "@myparcel/vue-form-builder": "^1.0.0-beta.1",
    "@vueuse/core": "^10.0.0",
    "pinia": "^2.1.7",
    "radash": "^11.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.17.6",
    "@myparcel-do/build-vite": "workspace:*",
    "@myparcel/semantic-release-config": "^5.0.0",
    "@pinia/testing": "^0.1.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.3",
    "@tailwindcss/container-queries": "^0.1.0",
    "@testing-library/vue": "^8.0.1",
    "@types/leaflet": "^1.9.4",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.1.0",
    "cssnano": "^6.0.3",
    "happy-dom": "^12.10.3",
    "histoire": "^0.17.6",
    "leaflet": "^1.9.4",
    "semantic-release": "^23.0.0",
    "semantic-release-monorepo": "^8.0.1",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-plugin-custom-tsconfig": "^1.0.0",
    "vite-plugin-singlefile": "^1.0.0",
    "vitest": "^1.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
