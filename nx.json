{
  "defaultBase": "main",
  "npmScope": "myparcel-do",
  "targets": {
    "build": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build:dev": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "test:run": {
      "outputs": [
        "{projectRoot}/coverage"
      ]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "build:dev",
          "test:run",
          "typecheck"
        ]
      }
    }
  }
}
