{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "preserveValueImports": false,
    "importsNotUsedAsValues": "remove",
    "types": [
      "vite/client"
    ],
    "paths": {
      "@myparcel/delivery-options-build-*": [
        "./packages/build/*/src"
      ],
      "@myparcel/delivery-options-*": [
        "./packages/*/src"
      ],
      "@myparcel/delivery-options": [
        "./packages/core/src"
      ]
    }
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
