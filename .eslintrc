{
  "root": true,
  "extends": [
    "@myparcel/eslint-config/preset-vue"
  ],
  "rules": {
    "jsdoc/no-undefined-types": [
      "warn",
      {
        "definedTypes": [
          "MyParcel",
          "MyParcelDeliveryOptions",
          "Wrapper",
          "VNode",
          "webpack"
        ]
      }
    ],
    "vue/no-unregistered-components": [
      "error",
      {
        "ignorePatterns": [
          "RecursiveForm",
          "BCard",
          "BDropdownItem",
          "BInputGroupPrepend",
          "BInputGroupText",
          "BNavItemDropdown",
          "BSpinner",
          "BTab",
          "BTabs"
        ]
      }
    ]
  },
  "overrides": [
    {
      "extends": "@myparcel/eslint-config/preset-typescript",
      "files": [
        "**/*.ts"
      ]
    }
  ]
}
