{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "noEmitOnError": false,
    "paths": {
      "@myparcel/delivery-options": ["./apps/delivery-options/src"],
      "@myparcel-do/shared/testing": ["./libs/shared/src/__tests__"],
      "@myparcel-do/*": ["./libs/*/src"]
    },
    "types": ["node", "vite/client"]
  },
  "include": [],
  "exclude": [
    "**/*.snap",
    "**/*.spec.*",
    "**/*.test.*",
    "**/__mocks__",
    "**/__tests__",
    "**/__snapshots__",
    "**/dist",
    "**/node_modules"
  ]
}
